<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.web.WebView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="at.jp.tourplanner.view.tour.DetailedTourView"
            prefHeight="400.0" prefWidth="600.0">
    <VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
        <Label text="%TourDetails.label" style="-fx-font-weight: bold;"/>
        <TextArea fx:id="detailedTourDescription"
                  editable="false"
                  wrapText="true"
                  prefWidth="400"
                  prefHeight="300"/>
        <WebView fx:id="webViewMap" prefWidth="150" prefHeight="150" layoutX="30" layoutY="30"/>
        <Button onAction="#onBackClicked" text="%back.button"/>
    </VBox>
</AnchorPane>
